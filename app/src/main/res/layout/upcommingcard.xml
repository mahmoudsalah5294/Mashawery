<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#FFFFFF"
    app:cardCornerRadius="20dp"
    app:cardElevation="5dp"
    app:cardUseCompatPadding="true">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="392dp"
        android:layout_height="139dp"
        android:background="#EFEDED"
        app:layout_constraintBottom_toTopOf="@+id/to"
        app:layout_constraintTop_toTopOf="@+id/to">


        <TextView
            android:id="@+id/date"
            android:layout_width="80dp"
            android:layout_height="110dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="12dp"
            android:layout_marginEnd="302dp"
            android:layout_marginBottom="17dp"
            android:fontFamily="sans-serif-thin"
            android:text="TextView"
            android:textSize="30sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0" />

        <TextView
            android:id="@+id/time"
            android:layout_width="130dp"
            android:layout_height="19dp"
            android:layout_marginStart="132dp"
            android:layout_marginTop="15dp"
            android:layout_marginEnd="130dp"
            android:layout_marginBottom="105dp"
            android:text="TextView"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/from2"
            android:layout_width="150dp"
            android:layout_height="40dp"
            android:layout_marginStart="132dp"
            android:layout_marginTop="92dp"
            android:layout_marginEnd="110dp"
            android:layout_marginBottom="7dp"
            android:text="TextView"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/from"
            android:layout_width="150dp"
            android:layout_height="40dp"
            android:layout_marginStart="132dp"
            android:layout_marginTop="34dp"
            android:layout_marginEnd="110dp"
            android:layout_marginBottom="65dp"
            android:text="TextView"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_marginStart="90dp"
            android:layout_marginTop="12dp"
            android:layout_marginEnd="270dp"
            android:layout_marginBottom="95dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_time" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_marginStart="90dp"
            android:layout_marginTop="53dp"
            android:layout_marginEnd="270dp"
            android:layout_marginBottom="54dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/date"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_outline_location_on_24" />

        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="57dp"
            android:layout_height="97dp"
            android:layout_marginStart="98dp"
            android:layout_marginTop="26dp"
            android:layout_marginEnd="237dp"
            android:layout_marginBottom="16dp"
            android:background="#00FFFFFF"
            android:rotation="90"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_baseline_horizontal_rule_24" />

        <Button
            android:id="@+id/Startbutton"
            android:layout_width="89dp"
            android:layout_height="44dp"
            android:layout_marginStart="290dp"
            android:layout_marginTop="13dp"
            android:text="Start"
            android:textSize="14sp"
            app:backgroundTint="#00BCD4"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/notes"
            android:layout_width="32dp"
            android:layout_height="31dp"
            android:layout_marginStart="352dp"
            android:layout_marginBottom="20dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/ic_outline_sticky_note_2_24" />

        <ImageView
            android:id="@+id/Delete"
            android:layout_width="31dp"
            android:layout_height="31dp"
            android:layout_marginStart="288dp"
            android:layout_marginTop="88dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_outline_delete_24" />

        <ImageView
            android:id="@+id/Edit"
            android:layout_width="31dp"
            android:layout_height="31dp"
            android:layout_marginTop="88dp"
            android:layout_marginEnd="40dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_outline_edit_24" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.cardview.widget.CardView>